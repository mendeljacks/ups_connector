{
    "name": "ups_connector",
    "version": "1.0.2",
    "description": "Autogenerated stateless connector for UPS api",
    "main": "build/index.js",
    "scripts": {
        "fetch": "nodemon --exec \"node -r sucrase/register -e \\\"require('./src/fetch.ts').fetch()\\\"\" --ext ts",
        "generate": "nodemon --exec \"node -r sucrase/register -e \\\"require('./src/generate.ts').generate()\\\"\" --ext ts --ignore index.ts",
        "prepare": "husky install",
        "bump": "tsc && npm version patch --no-git-tag-version && git add package*.json && npm publish"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^17.0.23",
        "axios": "^0.26.1",
        "husky": "^7.0.4",
        "sucrase": "^3.21.0"
    },
    "nodemonConfig": {
        "watch": [
            "src/"
        ]
    }
}
